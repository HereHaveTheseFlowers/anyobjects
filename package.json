{
  "name": "anyobjects",
  "version": "0.0.0",
  "description": "Curated project about Russian production. We have created an archive of objects that we found beautiful.",
  "scripts": {
    "bundle-report": "webpack-bundle-analyzer --port 4200 ./dist/bundle.js",
    "clean": "rimraf ./dist/",
