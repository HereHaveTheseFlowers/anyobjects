    "css-loader": "^6.7.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "modern-normalize": "1.1.0",
    "postcss": "8.4.18",
    "postcss-loader": "7.0.1",
    "postcss-short": "^5.0.0",
    "postcss-sorting": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "rimraf": "3.0.2",
